    <xs:simpleType name="msuFilterItemTypeEnum">
        <xs:annotation>
            <xs:documentation>
                Field stores the itemType for MSU Procedure download service. Can be PROC - for procedures, MUCA - for Measures By Initiating CA, COA - for Corrective Actions,
                MUOT - Measures by Other CA , OBJ - for Objections, INFO - for Additional Info
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="PROC"/>
            <xs:enumeration value="MUCA"/>
            <xs:enumeration value="COA"/>
            <xs:enumeration value="MUOT"/>
            <xs:enumeration value="OBJ"/>
            <xs:enumeration value="INFO"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="msuItemStatusEnum">
        <xs:annotation>
            <xs:documentation>
                Item Status for MSU Procedure download
                If FilterItemType is Procedure (PROC) acceptable values - OPEN, CLOSED, OPEN_WITH_MEASURES, OPEN_WITH_OBJECTIONS
                If FilterItemType is Measure (MUCA or MUOT) acceptable values - TAKEN, JUSTIFIED, WITHDRAWN, UNJUSTIFIED, CLOSED
                If FilterItemType is COA acceptable values - REQUESTED, PERFORMED, NOT_PERFORMED
                If FilterItemType is OBJ acceptable values - PROPOSED,WITHDRAWN
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="OPEN"/>
            <xs:enumeration value="CLOSED"/>
            <xs:enumeration value="OPEN_WITH_MEASURES"/>
            <xs:enumeration value="OPEN_WITH_OBJECTIONS"/>
            <xs:enumeration value="TAKEN"/>
            <xs:enumeration value="JUSTIFIED"/>
            <xs:enumeration value="WITHDRAWN"/>
            <xs:enumeration value="UNJUSTIFIED"/>
            <xs:enumeration value="CLOSED"/>
            <xs:enumeration value="REQUESTED"/>
            <xs:enumeration value="PERFORMED"/>
            <xs:enumeration value="NOT_PERFORMED"/>
            <xs:enumeration value="PROPOSED"/>
            <xs:enumeration value="WITHDRAWN"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="MsuProcedureSearchCriteriaType">
        <xs:sequence>
            <xs:element name="FilterItemType" type="s:msuFilterItemTypeEnum" minOccurs="1" maxOccurs="1"/>
            <xs:element name="ItemIdentifier" type="xs:string" minOccurs="0" maxOccurs="1"/>
            <xs:element name="ItemStatus" type="s:msuItemStatusEnum" minOccurs="0" maxOccurs="1"/>
            <xs:element name="CAActorCode" type="commonbasic:stringXSType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="LocalCaReference" type="xs:string" minOccurs="0" maxOccurs="1"/>
            <xs:element name="EOSRN" type="commonbasic:stringSRNType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="EONAME" type="xs:string" minOccurs="0" maxOccurs="1"/>
            <xs:element name="State" type="commonbasic:registeredOrDiscardedState" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Only REGISTERED or DISCARDED</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="LastUpdateDateFrom" type="xs:dateTime" minOccurs="1" maxOccurs="1"/>
            <xs:element name="includeHistoricVersions" type="s:yesNoT" default="No" minOccurs="0" maxOccurs="1"/>
            <xs:element name="ProcedureType" type="proc:ProcedureTypeEnum" minOccurs="0" maxOccurs="1"/>
            <xs:element name="ProcedureTriggerType" type="proc:ProcedureTriggerTypeEnum" minOccurs="0" maxOccurs="1"/>
            <xs:element name="AffectedCountry" type="country:CountryEnum" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="CECPDownloadServiceCriteriaType">
        <xs:annotation>
            <xs:documentation>
                Only one value per criterion will be accepted. No wildcards.
                "AND" condition applied between search criterion.
                When requesting M2M or bulk download of CECPP records the system will ensure that at least one criterion is provided.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="s:ServiceCriteriaType">
                <xs:all>
                    <xs:element name="CECPStatusDateChange" type="commonbasic:DateInterval" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                #FLD-CRF-306#
                                Indicates the date when the last status update has occurred.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ApplicationID" type="commonbasic:stringXSType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                #FLD-CRF-302#
                                ID of the application for conformity assessment
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ConformityAssessmentType" type="links:CertificateTypeEnum" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                #FLD-CRF-310#
                                Defines (enumeration) the conformity assessment procedure (certificate type) of the device in the CECP. Only the following conformity assessment procedures may have a CECP:
                                    (MDR) EU technical documentation assessment (Annex IX Chapter II);
                                    (MDR) EU type-examination (Annex X);
                                    (MDR) EU Quality Management System certificate (Annex IX Chapter I)
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="NotifiedBodyID" type="commonbasic:stringXSType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                #FLD-CRF-307#
                                The NB Identification number (assigned by NANDO) of the NB carrying out the conformity assessment of the device in the CECP.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="EOSRN"  type="commonbasic:stringSRNType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                #FLD-CRF-308#
                                The SRN of the MF of the device in the clinical evaluation (CECP).  The MF is identified by its SRN (a unique identification number assigned to the MF when it is registered as an Actor in Eudamed).
                                =======
                                #FLD-CRF-309#
                                The SRN of the AR of the device in the clinical evaluation (CECP). It applies only when the MF of the device is a non-EU MF. The AR is identified by its SRN (a unique identification number assigned to the AR when it is registered as an Actor in Eudamed).
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="DeviceIdentification"  type="crtfcecp:DeviceIdentification" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                #FLD-CRF-322#
                                The Basic UDI-DI of the device in the scope of the CECP, if the device has been assigned a Basic UDI-DI.
                                Notes
                                The Basic UDI-DI has been assigned in case the CECP is performed in the context of a:
                                (MDR) EU technical documentation assessment (Annex IX Chapter II);
                                (MDR) EU type-examination (Annex X).
                                    The Basic UDI-DI has been assigned in case the CECP is performed in the context of a (MDR) EU Quality Management System certificate (Annex IX Chapter I)
                                    ========
                                    FLD-CRF-326
                                    The name of the device in the scope of the CECP, if the device has not been assigned a Basic UDI-DI.
                                    ========
                                    FLD-CRF-325
                                    The model of the device in the CECP.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="DeviceType"  type="riskclass:RiskClassEnum" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                #FLD-CRF-324#
                                The risk class of the device in the CECP.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Status" type="crtfcecp:CECPFollowedStatusEnum" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                #FLD-CRF-311#
                                Indicates (enumeration) the workflow status of the CECP (for example if the clinical evaluation consultation procedure applies, if the CECP has been assigned to an Expert Panel, etc.).
                                Note: This information is additional internal Eudamed information for each CECP. A CECP has only one version i.e. the versionNumber in entity VersionData is always 1.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:all>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

</xs:schema>
