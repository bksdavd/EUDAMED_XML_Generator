    <xs:complexType name="LanguageSpecificTextType">
        <xs:sequence>
            <xs:element name="language" type="lsn:LanguageEnum" minOccurs="1" maxOccurs="1"/>
            <xs:element name="textValue" type="commonbasic:stringLType" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>The value NA can be exported for empty values related to the mandate termination</xs:documentation>
