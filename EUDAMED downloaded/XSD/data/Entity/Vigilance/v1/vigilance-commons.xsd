    <xs:complexType name="DossierType">
        <xs:complexContent>
            <xs:restriction base="vigbase:AbstractDossierType">
                <xs:group ref="vigbase:DossierGroup"/>
                <xs:attribute name="version" type="xs:string" use="required" fixed="1"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="FileType">
        <xs:sequence>
            <xs:element name="FileName" type="xs:string"/>
            <xs:element name="FileURI" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Provide a unique reference for this file inside the archive sent with the payload. Ex. [DossierExternalId]/[fileName].pdf.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ActionDetailsType1">
        <xs:complexContent>
            <xs:extension base="vigbase:ActionDetailsType">
                <xs:sequence>
                    <xs:element name="nbEvaluation" type="xs:string" minOccurs="0"/>
                    <xs:element name="nbContactPersonDetails" type="xs:string" minOccurs="0"/>
                    <xs:element name="nbContactFirstName" type="xs:string" minOccurs="0"/>
                    <xs:element name="nbContactLastName" type="xs:string" minOccurs="0"/>
                    <xs:element name="contactEmail">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:annotation>
                                    <xs:documentation>email regex</xs:documentation>
                                </xs:annotation>
                                <xs:pattern value="^[\w\-.]+@([\w\-]+\.)+[\w\-]{2,4}$"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="contactPhone" minOccurs="0">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:annotation>
                                    <xs:documentation>phone number regex</xs:documentation>
                                </xs:annotation>
                                <xs:pattern value="^[+]?[(]?[0-9]{3}[)]?[\-\s.]?[0-9]{3}[\-\s.]?[0-9]{4,6}$"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="psurEvaluation" type="xs:string" minOccurs="0"/>
                    <xs:element name="evaluationReferenceNumber" type="xs:string" minOccurs="0"/>
                    <xs:element name="evaluationDecision" type="ENUM_PSUR_NB_EVALUATION_DECISION"/>
                    <xs:element name="evaluationDocument" type="FileType" minOccurs="0"/>
                    <xs:element name="additionalInformationAndOrActionsRequestedByNb" type="xs:string" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="ActionDetailsType2">
        <xs:complexContent>
            <xs:extension base="vigbase:ActionDetailsType">
                <xs:sequence>
                    <xs:element name="attachment" type="FileType" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="ActionDetailsType3">
        <xs:complexContent>
            <xs:extension base="vigbase:ActionDetailsType">
                <xs:sequence>
