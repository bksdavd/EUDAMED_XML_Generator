                    <xs:documentation>SOFTWARE</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="IVDD_SOFTWARE">
                <xs:annotation>
                    <xs:documentation>SOFTWARE</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="OCMIVDSpecialDeviceTypeEnum">
        <xs:annotation>
            <xs:documentation>
                - Software
