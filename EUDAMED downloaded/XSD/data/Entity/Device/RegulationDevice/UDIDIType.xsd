<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xsl" href="/tools/eudamed/dtx/xslt/index.xsl"?>
<xs:schema targetNamespace="https://ec.europa.eu/tools/eudamed/dtx/datamodel/Entity/UDIDI/v1"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:udidi="https://ec.europa.eu/tools/eudamed/dtx/datamodel/Entity/UDIDI/v1"
           xmlns:e="https://ec.europa.eu/tools/eudamed/dtx/datamodel/Entity/v1"
           xmlns:commondi="https://ec.europa.eu/tools/eudamed/dtx/datamodel/Entity/Device/CommonDevice/v1"
           xmlns:marketinfo="https://ec.europa.eu/tools/eudamed/dtx/datamodel/Entity/MktInfo/MarketInfo/v1"
           xmlns:commonbasic="https://ec.europa.eu/tools/eudamed/dtx/datamodel/CommonBasic/v1"
           xmlns:links="https://ec.europa.eu/tools/eudamed/dtx/datamodel/Entity/Links/v1"
           xmlns:lsn="https://ec.europa.eu/tools/eudamed/dtx/datamodel/Entity/Common/LanguageSpecific/v1"
           xmlns:party="https://ec.europa.eu/tools/eudamed/dtx/datamodel/Party/v1"
           xmlns:address="https://ec.europa.eu/tools/eudamed/dtx/datamodel/Entity/Party/Address/v1"
           elementFormDefault="qualified" version="1.0">
    <xs:import namespace="https://ec.europa.eu/tools/eudamed/dtx/datamodel/Party/v1" schemaLocation="../../Party/Party.xsd"/>
    <xs:import namespace="https://ec.europa.eu/tools/eudamed/dtx/datamodel/Entity/Common/LanguageSpecific/v1" schemaLocation="../../Common/LanguageSpecificNameType.xsd"/>
    <xs:import namespace="https://ec.europa.eu/tools/eudamed/dtx/datamodel/Entity/Links/v1" schemaLocation="../../Links/LinkType.xsd"/>
    <xs:import namespace="https://ec.europa.eu/tools/eudamed/dtx/datamodel/CommonBasic/v1" schemaLocation="../../Common/CommonBasicType.xsd"/>
    <xs:import namespace="https://ec.europa.eu/tools/eudamed/dtx/datamodel/Entity/MktInfo/MarketInfo/v1" schemaLocation="../../MarketInfo/MarketInfoType.xsd"/>
    <xs:import namespace="https://ec.europa.eu/tools/eudamed/dtx/datamodel/Entity/Device/CommonDevice/v1" schemaLocation="../CommonDeviceType.xsd"/>
    <xs:import namespace="https://ec.europa.eu/tools/eudamed/dtx/datamodel/Entity/v1" schemaLocation="../../Entity.xsd"/>
    <xs:import namespace="https://ec.europa.eu/tools/eudamed/dtx/datamodel/Entity/Party/Address/v1" schemaLocation="../../Party/AddressType.xsd"/>
    <xs:complexType name="SubstancesType">
        <xs:sequence>
            <xs:element name="substance" type="udidi:SubstanceType" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="AnnexXVINonMedicalDeviceTypesType">
        <xs:sequence>
            <xs:element name="nmdType" type="udidi:NonMedicalDeviceEnum" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ProductDesignerOrganisationType">
        <xs:complexContent>
            <xs:extension base="party:PartyType">
                <xs:sequence>
                    <xs:element name="organizationName" type="lsn:LanguageOptionalTextType" minOccurs="1" maxOccurs="1"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="ProductDesignerActorType">
        <xs:complexContent>
            <xs:extension base="e:Entity">
                <xs:sequence>
                    <xs:choice minOccurs="1" maxOccurs="1">
                        <xs:element name="productDesignerActorCode" type="commonbasic:stringSRNType" minOccurs="1" maxOccurs="1"/>
                        <xs:element name="productDesignerOrganisation" type="udidi:ProductDesignerOrganisationType" minOccurs="1" maxOccurs="1"/>
                    </xs:choice>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="DeviceUDIDIDataType" abstract="true">
        <xs:complexContent>
            <xs:extension base="udidi:UDIDIDataType">
                <xs:sequence>
                    <xs:element name="numberOfReuses" type="commonbasic:integer-1ToMax" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                #FLD-UDID-157# - Maximum number of reuses. Property defines the Maximum Number of reuses applicable for the Device.
                                Use 1 value when the maximum number of reuses is not defined (Maximum number of reuses are not applicable), 0 if the Device is considered Single-use device and
                                a positive number when the Device has a limited number of reuses (provided number being the number of reuses applicable).
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="relatedUDILink" type="links:DeviceRelatedLinkType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="marketInfos" type="marketinfo:MarketInfosType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="deviceMarking" type="udidi:DeviceMarkingType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                Element contains the information regarding the Direct Marking DI or Unit of Use DI referencing the Device.
                                Attribute can be provided for Regulation Devices (from the first registration or later in the update of the UDI) and must not be provided for Legacy Devices.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="baseQuantity" type="commonbasic:positiveInteger" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                #FLD-UDID-151# - Quantity of Device. The base quantity in which the Device is provided.
                                Attribute is required for Regulation Devices and must not be provided for Legacy Devices.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="productDesignerActor" type="udidi:ProductDesignerActorType" minOccurs="0" maxOccurs="1"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:simpleType name="StringsType">
        <xs:list itemType="xs:string"/>
    </xs:simpleType>
    <xs:complexType name="DeviceMarkingType">
        <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="1">
                <xs:element name="directMarkingDI" type="commondi:DIIdentifierType" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>#FLD-UDID-138# - Direct Marking UDI-DI.
                            Direct Marking UDI-DI - is the DI with which the Device (UDI-DI) is marked directly. Field is mandatory when the Device is directly marked (Direct Marking = YES).
                            The value of the field is a DI - either the current UDI-DI (of the current device) or a different UDI-DI.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="unitOfUseIdentifier" type="commondi:DIIdentifierType" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>
                            #FLD-UDID-135# - Unit of Use DI. Reference to the Unit of Use DI applicable for the current UDI-DI (Device Identifier).
                            A virtual identifier assigned to an individual medical device when a Unique Device Identifier (UDI) is not labeled on the individual device at the level of its unit of use.
                            Its purpose is to associate the use of a device to/on a patient when a base package contains more than one device.
                            Field is mandatory when the value of the field Direct marking is false and Quantity of Device is more than 1.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="MedicalHumanProductSubstanceType">
        <xs:complexContent>
            <xs:extension base="udidi:SubstanceType">
                <xs:sequence>
                    <xs:element name="INN" type="commonbasic:stringLType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                #FLD-UDID-204# - INN. Field stores the INN value for the substance provided.
                                INN can be provided only for CMR or Endocrine-disrupting substances which, if used separately, may be considered to be a medicinal product or substances which, if used separately,
                                may be considered to be a medicinal product derived from human blood or human plasma.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="type" type="commondi:MedicalHumanSubstanceTypeEnum" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                #FLD-UDID-196# - Labeled for presence of substances being CMR of category 1A or 1B / Labeled for presence of substances having endocrine-disrupting properties/ Presence of a substance which,
                                if used separately, may be considered to be a medicinal product/ Presence of a substance which, if used separately, may be considered to be a medicinal product derived from human blood or human plasma.
                                When registering a new Device containing in its composition substances (substances being CMR of category 1A or 1B / substances having endocrine-disrupting properties/ substances which, if used separately,
                                may be considered to be a medicinal product/ substances which, if used separately, may be considered to be a medicinal product derived from human blood or human plasma),
                                Manufacturer will have to submit the details of those substances.
                                Field stores the details regarding the type of Substance registered (CMR Substance / Endocrine substance/ Substance
                                which can be considered a Medicinal Product or Substance which may be considered a medicinal product derived from human blood or plasma).
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="IVDRUDIDIsType">
        <xs:sequence>
            <xs:element name="UDIDI" type="udidi:IVDRUDIDIDataType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="MDRUDIDIsType">
        <xs:sequence>
            <xs:element name="UDIDI" type="udidi:MDRUDIDIDataType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="EndocrineSubstanceType">
        <xs:complexContent>
            <xs:extension base="udidi:SubstanceType">
                <xs:sequence minOccurs="0">
                    <xs:element name="CASCode" type="commonbasic:stringCASType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                #FLD-UDID-202# - CAS. Field stores the CAS Code (CAS#) associated to the CMR or Endocrine disrupting substance.
                                CAS# can be provided only for CMR or Endocrine-disrupting substances.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ECCode" type="commonbasic:stringECCODEType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>#FLD-UDID-203# - EC#. Field Stores the EC Code (EC#)associated to the CMR or Endocrine disrupting substance.
                                EC# can be provided only for CMR or Endocrine-disrupting substances.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="PRUDIDIDataType">
        <xs:complexContent>
            <xs:extension base="udidi:UDIDIDataType">
                <xs:sequence/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="UDIDIDataType" abstract="true">
        <xs:annotation>
            <xs:documentation>UDI DI data (alias Product or Device) entity stores information and characteristics of the UDI DI.
                It is a generalization of the UDIDIType Class (it inherits the attributes of the UDIDIType class).
                The UDI DI also has an association relationship with the ContainedItem in order to keep the relation between the UDI DI and the Package DI
                It is connected to the BASIC UDI DI class (containing the information`s and characteristics of the Device Model BASIC UDI DI ) - a Basic UDI DI can have several UDI DI`s.
                Contains references to Clinical Size Class (for storing details about Device Clinical Sizes), CMR Substance Class (for storing information`s related to CMR Substances),
                Organization (for storing information`s related to original Manufacturer), MarketInfoLink (Information about Countries were the Device is Made Available on the Market).
                UDI DI is versioned - contains versionData
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="udidi:UDIDIType">
                <xs:sequence>
                    <xs:element name="additionalDescription" type="lsn:LanguageSpecificTextsType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                #FLD-UDID-175# - Additional product Description.
                                Additional Product Description. Additional Product Description is required for System or Procedure Packs or for Standard Devices being marked as System or Procedure Packs (as a Device in themselves).
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="basicUDIIdentifier" type="commondi:DIIdentifierType" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>#FLD-UDID-145# - Basic UDI-DI Identifier. Reference to the Basic UDI-DI to which the Device is associated.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="MDNCodes" type="udidi:StringsType" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                #FLD-UDID-149# - Nomenclature code.
                                The Device Nomenclature Code(s) associated with the UDI-DI. The corresponding EMDN Code(s) will be provided/returned in the field. Several EMDN codes can be provided for a Device.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="productionIdentifier" type="udidi:PIElementEnumType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                #FLD-UDID-148# - Type of UDI-PI.
                                Reference to the Types of UDI-PI (production Identifiers) applicable to the current Device (UDI-DI).
                                Defines the way the production is controlled. Attribute is required for Regulation Devices and System or Procedure Packs and must not be provided for Legacy Devices.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="referenceNumber" type="commonbasic:stringSType" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>#FLD-UDID-163# - Reference / Catalogue Number. Property stores the Reference or Catalogue Number of the Device.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="secondaryIdentifier" type="commondi:DIIdentifierType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>#FLD-UDID-136# - Secondary UDI - DI value. Secondary UDI DI Code - reference to the Secondary UDI-DI registered for the current UDI-DI . Additional DI associated to this UDI-DI.
                                Only one Secondary UDI-DI (Additional UDI-DI) can be associated to a UDI-DI.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="sterile" type="xs:boolean" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>#FLD-UDID-169# - Device labeled sterile. Property defines if the UDI-DI is labeled as Sterile or not.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="sterilization" type="xs:boolean" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>#FLD-UDID-170# - Need for sterilisation before use. Property defines if the UDI-DI needs to be sterilised before use or not.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="tradeNames" type="lsn:LanguageSpecificTextsType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                #FLD-UDID-176# - Trade name.
                                Device Trade Name assigned to the UDI-DI. Trade names can be provided if applicable for the device. Several Trade- Names can be provided for the Device in the same language or in different languages.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="website" type="commonbasic:stringSType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>#FLD-UDID-174# - URL for additional information. Manufacturer website where additional information about the device (UDI-DI) are available.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="storageHandlingConditions" type="commondi:StorageHandlingConditionsType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="packages" type="udidi:PackageUDIsType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                Element will contain the details of the Packages structure for the current Device(UDI-DI).
                                Information about Package structure must not be provided for Legacy Devices.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="criticalWarnings" type="commondi:CriticalWarningsType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="substatuses" type="commondi:SubstatusesType" minOccurs="0" maxOccurs="1"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="UDIDIType" abstract="true">
        <xs:annotation>
            <xs:documentation>UDIDI Class stores the Identification information attributes for the UDI DI and Package Level DI and the Status of the UDI DI and of the Container Package Information.
                It creates and manages the relationship between the UDI DI and the different structures of Container packaging Information.
                Classes attributes are contained in the UDI DI class (through the generalization relationship).
                The self reference is used to express (to define) the Package structure - each new Package element added will reference it`s parent (the previous Package element added).
                In the case of the first package structure element, the element referenced will be the UDI DI.
                UDIDI entity is versiond independently from the UDI DI - contains versionData
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="e:Entity">
                <xs:sequence>
                    <xs:element name="identifier" type="commondi:DIIdentifierType" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="status" type="commondi:UDIDIStatusType" minOccurs="1" maxOccurs="1"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="MDRUDIDIDataType">
        <xs:complexContent>
            <xs:extension base="udidi:DeviceUDIDIDataType">
                <xs:sequence>
                    <xs:element name="annexXVINonMedicalDeviceTypes" type="udidi:AnnexXVINonMedicalDeviceTypesType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                #FLD-UDID-147# - Intended purpose other than medical (Annex XVI).
                                Field stores the values regarding the Intended purpose of the Device, other the medical, as they are described in the Annex XVI of MDR Regulation.
                                Field can be provided for Regulation Devices (if applicable) and must not be provided for Legacy Devices.
                                Not available for a Master UDI-DI
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="annexXVIApplicable" type="xs:boolean" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                #BR-DTX-UDID-730: Master UDI-DI properties applicability.
                                Available only for a Master UDI-DI
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="latex" type="xs:boolean" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>#FLD-UDID-156# - Containing latex. Property defines if the Device contains latex into it`s composition.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="reprocessed" type="xs:boolean" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>#FLD-UDID-164# - Reprocessed single use device. Property defines if the Device is a Reprocessed single use device or not.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="substances" type="udidi:SubstancesType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="clinicalSizes" type="commondi:ClinicalSizesType" minOccurs="0" maxOccurs="1"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="IVDRUDIDIDataType">
        <xs:complexContent>
            <xs:extension base="udidi:DeviceUDIDIDataType">
                <xs:sequence>
                    <xs:element name="newDevice" type="xs:boolean" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                #FLD-UDID-159# - New Device.
                                Defines if the Device can be considered a New Device or not.
                                A device shall be considered to be new if:
                                (a) there has been no such device continuously available on the Union market during the previous three years for the relevant analyte or other parameter
                                (b) the procedure involves analytical technology not continuously used in connection with a given analyte or other parameter on the Union market during the previous three years.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:simpleType name="PIElementEnumType">
        <xs:list itemType="udidi:PIElementEnum"/>
    </xs:simpleType>
    <xs:complexType name="PackageUDIType">
        <xs:complexContent>
            <xs:extension base="udidi:UDIDIType">
                <xs:sequence>
                    <xs:element name="child" type="commondi:DIIdentifierType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                #FLD-UDID-124# - Container Packaging Structure is a hierarchical (descending list) structure defining how the Device is packaged together for shipping and selling reasons.
                                The basic (the lower) element of the Packaging Structure is the UDI-DI. The field stores a link to next child element in the Package Structure.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="numberOfItems" type="xs:positiveInteger" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>#FLD-UDID-121# - Quantity per package. Number of Items in the Package Structure.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="SubstanceType" abstract="true">
        <xs:annotation>
            <xs:documentation>
                Class that references the Substances registered in the Device.
                Substances can be declared either by entering the Name in several Languages or by declaring the INN (International Nonproprietary Name) Code
                If declaring the substance using free text - language will be provided. Same Substance can be mentioned in several Languages
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="names" type="lsn:LanguageOptionalTextsType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        #FLD-UDID-201# - Name of Substance.
                        Field stores the Name of Substance (applicable for all types of substances - CMR, Endocrine, Medicinal product, Medicinal product derived from human blood or human).
                        In case INN information is provided (for substances being Medicinal product, Medicinal product derived from human blood or human), the field is no longer required - must not be provided.
                        In case CASCode or ECCode are provided (for substances being CMR or Endocrine substances), Name of substance can be provided only once - having the Language ANY
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CMRSubstanceType">
        <xs:complexContent>
            <xs:extension base="udidi:SubstanceType">
                <xs:sequence minOccurs="0">
                    <xs:element name="CASCode" type="commonbasic:stringCASType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                #FLD-UDID-202# - CAS. Field stores the CAS Code (CAS#) associated to the CMR or Endocrine disrupting substance.
                                CAS# can be provided only for CMR or Endocrine-disrupting substances.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ECCode" type="commonbasic:stringECCODEType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                #FLD-UDID-203# - EC#. Field Stores the EC Code (EC#)associated to the CMR or Endocrine disrupting substance.
                                EC# can be provided only for CMR or Endocrine-disrupting substances.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="type" type="udidi:CMRSubstanceTypeEnum" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                #FLD-UDID-200# - Category of CMR
                                Field stores the Type of CMR Substance (1A or 1B). It is only applicable when the Substance registered is a CMR Substance
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="PackageUDIsType">
        <xs:complexContent>
            <xs:extension base="e:Entity">
                <xs:sequence>
                    <xs:element name="package" type="udidi:PackageUDIType" minOccurs="1" maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="DTXPackageUDIType">
        <xs:sequence>
            <xs:element name="udiIdentifier" type="commondi:DIIdentifierType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="packages" type="udidi:PackageUDIsType" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="IssuingEntityTypeEnum">
        <xs:annotation>
            <xs:documentation>
                Issuing entities of the UDI Codes.
                The issuing Entities of the UDI Codes will be established by the European Commission through implementing acts
            </xs:documentation>
        </xs:annotation>
