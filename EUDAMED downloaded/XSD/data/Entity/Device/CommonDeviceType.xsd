            <xs:documentation>Enumeration presenting the Size Types taken into consideration for a Device</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="CST1">
                <xs:annotation>
                    <xs:documentation>Acidity</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="CST40">
                <xs:annotation>
                    <xs:documentation>Addition power (Optical)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="CST2">
                <xs:annotation>
                    <xs:documentation>Amount of fingers</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="CST3">
                <xs:annotation>
                    <xs:documentation>Angle</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="CST58">
                <xs:annotation>
                    <xs:documentation>Back cylinder axis</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="CST57">
                <xs:annotation>
                    <xs:documentation>Back cylinder power</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="CST18">
                <xs:annotation>
                    <xs:documentation>Balloon length (nominal)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="CST42">
                <xs:annotation>
                    <xs:documentation>Base curve (B.C. or R0)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="CST4">
                <xs:annotation>
                    <xs:documentation>Bevel</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="CST17">
                <xs:annotation>
                    <xs:documentation>Body side</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="CST56">
                <xs:annotation>
                    <xs:documentation>Body weight [kg]</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="CST6">
                <xs:annotation>
                    <xs:documentation>Cannula wall</xs:documentation>
                </xs:annotation>
