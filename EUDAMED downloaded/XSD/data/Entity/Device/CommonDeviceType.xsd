                    <xs:element name="text" type="commonbasic:stringSType" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>#FLD-UDID-194# - Value(Text). Value that the Clinical Size can get (when Other is selected as precision).</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
