    <xs:complexType name="SPPGroupType">
        <xs:sequence>
            <xs:element name="sppGroupIdentification" type="xs:string" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="SterilisationMethodsEnum">
        <xs:restriction base="xs:string">
            <xs:enumeration value="ASEPTIC_PROCESSING">
                <xs:annotation>
                    <xs:documentation>Aseptic processing</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="EOG">
                <xs:annotation>
                    <xs:documentation>Ethylene oxide gas sterilisation (EOG)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="LOW_TEMPERATURE">
                <xs:annotation>
                    <xs:documentation>Low temperature steam and formaldehyde sterilisation</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="MOIST_HEAT">
                <xs:annotation>
                    <xs:documentation>Moist heat sterilisation</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="RADIATION">
                <xs:annotation>
                    <xs:documentation>Radiation sterilisation (gamma, x-ray, electron beam)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="OTHERS"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="SystemProcedurePackSterilisationType">
        <xs:sequence>
            <xs:element name="spppSteriliser" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
            <xs:element name="sppGroupApplicable" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
            <xs:element name="comments" type="comment:CommentsType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="sterilisationMethod" type="crtfscp:SterilisationMethodsEnum" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="steriliser" type="party:OrganisationType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="sppGroup" type="crtfscp:SPPGroupType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SpecialDeviceType">
        <xs:sequence>
            <xs:element name="deviceManufTissueAnimal" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
            <xs:element name="deviceManufTissueHuman" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
            <xs:element name="deviceInSterileCondition" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
            <xs:element name="deviceInVitro" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
            <xs:element name="deviceWithoutMedPurpose" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="QualityProcedureScopeType">
        <xs:sequence>
            <xs:element name="riskClass" type="riskclass:RiskClassEnum" minOccurs="0" maxOccurs="4"/>
            <xs:element name="systemProcedurePackSterilisation" type="crtfscp:SystemProcedurePackSterilisationType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Defines the information that needs to be provided to include a system or procedure pack sterilisation (SPPS) in the scope of a MDR quality conformity assessment procedure.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="model" type="commonbasic:stringXSType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="name" type="commonbasic:stringSType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="basicUdiKnown" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
            <xs:element name="implantable" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
            <xs:element name="isRegisteredInUDIModule" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
            <xs:choice minOccurs="0" maxOccurs="1">
                <xs:element name="DICode" type="commondi:DIIdentifierType" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>
                            Basic UDI-DI of a device in the scope of a quality conformity assessment procedure.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="CMDdescription" type="commonbasic:stringLType" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>
                            Description (free text) of a custom-made device (CMD) in the scope of a MDR quality conformity assessment procedure.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="deviceReference" type="commonbasic:stringMType" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>
                            Device reference/catalogue number in the scope of a quality conformity assessment procedure.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="deviceGroupIdentifier" type="commonbasic:stringLType" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>
                            Identification (free text) of a device group in the scope of a quality conformity assessment procedure.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="deviceName" type="commonbasic:stringMType" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>
                            Device name in the scope of a quality conformity assessment procedure.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:choice>
            <xs:element name="classICharacteristics" type="crtfscp:QualityCertificatePropertiesType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Additional (1 to 3) device characteristics (enumeration) for a class I device in the scope of a quality conformity assessment procedure.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="wasCECPfollowed" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
            <xs:element name="mosOutsideEudamed" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
